{
  "name": "llm-excel",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:api": "pnpm --filter @llm-excel/api dev",
    "dev:docker": "docker compose -f docker-compose.dev.yml up -d",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.7.5",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=22"
  },
  "volta": {
    "node": "22.22.0",
    "pnpm": "10.28.0"
  }
}